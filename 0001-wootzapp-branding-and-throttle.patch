index 0a7c7b7fed1ed..b89a0a8857aba 100644
--- a/chrome/android/BUILD.gn
+++ b/chrome/android/BUILD.gn
@@ -2589,7 +2589,7 @@ if (current_toolchain == default_toolchain) {
 
   chrome_public_apk_or_module_tmpl("chrome_public_apk") {
     target_type = "android_apk"
-    apk_name = "ChromePublic"
+    apk_name = "WootzApp"
     art_profile_path = "//chrome/android/baseline_profiles/profile.txt"
     if (android_64bit_target_cpu) {
       # Ensure 64-bit chrome does not depend on 32-bit things.
@@ -2600,7 +2600,7 @@ if (current_toolchain == default_toolchain) {
   }
 
   chrome_public_bundle("chrome_public_bundle") {
-    bundle_name = "ChromePublic"
+    bundle_name = "WootzApp"
   }
 
   android_library("monochrome_java") {
@@ -3064,7 +3064,7 @@ if (current_toolchain == default_toolchain) {
     android_test_apk = ":chrome_smoke_test_apk"
     additional_apks = [ "//net/android:net_test_support_apk" ]
     if (!is_java_debug) {
-      proguard_mapping_path = "$root_build_dir/apks/ChromePublic.apk.mapping"
+      proguard_mapping_path = "$root_build_dir/apks/WootzApp.apk.mapping"
     }
 
     extra_args = _common_smoke_test_args
@@ -3283,7 +3283,7 @@ if (current_toolchain == default_toolchain) {
       "print-reftest",
       "--zero-tests-executed-ok",
       "--browser-apk",
-      "@WrappedPath(apks/ChromePublic.apk)",
+      "@WrappedPath(apks/WootzApp.apk)",
     ]
     data = [
       "//third_party/blink/web_tests/platform/android/",
