+import("//chrome/common/features.gni")
+import("//tools/grit/grit_rule.gni")
+import("//tools/grit/preprocess_if_expr.gni")
+import("//ui/webui/resources/tools/generate_grd.gni")
+
+preprocess_folder = "preprocessed"
+preprocess_manifest = "preprocessed_manifest.json"
+
+grit("resources") {
+  defines = chrome_grit_defines
+
+  enable_input_discovery_for_gn_analyze = false
+  source = "$target_gen_dir/resources.grd"
+  deps = [ ":build_grd" ]
+  outputs = [
+    "grit/wootzapp_throttle_resources.h",
+    "grit/wootzapp_throttle_resources_map.h",
+    "grit/wootzapp_throttle_resources_map.cc",
+    "wootzapp_throttle_resources.pak",
+  ]
+  output_dir = "$root_gen_dir/chrome"
+}
+
+generate_grd("build_grd") {
+  grd_prefix = "wootzapp_throttle"
+  out_grd = "$target_gen_dir/resources.grd"
+  input_files = [
+    "wootzapp_browser_bridge.js",
+    "wootzapp_index.js",
+    "wootzapp_main.css",
+    "wootzapp_throttle_view.js",
+    "wootzapp_util.js",
+    "wootzapp_view.js",
