<layer-list
xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:width="@dimen/tab_group_color_icon_item_size"
        android:height="@dimen/tab_group_color_icon_item_size"
        android:gravity="center">
        <shape
            android:shape="oval">
            <solid android:color="@color/gm3_baseline_surface_light"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:bottom="@dimen/tab_group_color_icon_inset"
        android:left="@dimen/tab_group_color_icon_inset"
        android:right="@dimen/tab_group_color_icon_inset"
        android:top="@dimen/tab_group_color_icon_inset">
        <shape
            android:shape="oval">
            <solid android:color="@macro/default_icon_color"/>
        </shape>
    </item>
</layer-list>